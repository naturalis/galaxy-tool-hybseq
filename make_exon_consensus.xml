<tool id="mec" name="Make_exon_consensus">
  <description> create exon consensus </description>
	<command><![CDATA[
    bash 'python' '{__tool_directory__}/Make_exon_consensus.py' 'input_fasta'
 	]]></command>
	<inputs>
		<param name="input_fasta" type="data" format="fasta" label="input fasta file"/>
	</inputs>
	<outputs>
  		<data name="output" label="output consensus file" />
	</outputs>
	<help>
		This tool creates exon consensus from seperate exons
  </help>
</tool>